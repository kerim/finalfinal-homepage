{
  "name": "finalfinal-homepage",
  "version": "1.0.0",
  "description": "Landing page for FINAL|FINAL, the writing app for academics",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all build:css --parallel dev:*",
    "dev:eleventy": "eleventy --serve --watch",
    "dev:css": "tailwindcss -i src/style.css -o _site/css/style.css --watch",
    "build": "npm run build:css && eleventy",
    "build:css": "tailwindcss -i src/style.css -o _site/css/style.css --minify",
    "preview": "eleventy --serve"
  },
  "keywords": [
    "eleventy",
    "landing-page",
    "finalfinal",
    "writing-app"
  ],
  "author": "P. Kerim Friedman",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@tailwindcss/typography": "^0.5.15",
    "autoprefixer": "^10.4.20",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.47",
    "tailwind-scrollbar-hide": "^1.1.7",
    "tailwindcss": "^3.4.13"
  }
}
